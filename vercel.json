{
  "version": 2,
  "functions": {
    "api/server.js": {
      "maxDuration": 60
    }
  },
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/health",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/process-timeouts",
      "schedule": "*/2 * * * *"
    },
    {
      "path": "/api/cron/process-disparo-queue",
      "schedule": "* * * * *"
    }
  ]
}
